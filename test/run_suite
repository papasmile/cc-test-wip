wget https://dl.google.com/cloudsql/cloud_sql_proxy.linux.amd64 -O cloud_sql_proxy &&
chmod +x cloud_sql_proxy &&
./cloud_sql_proxy -instances=engineering-338720:us-central1:eng-db-1=tcp:3306 &&
node -e 'console.log(`Tests start ` + new Date())' &&
mocha ./test/getCampgroundTest.js --reporter-options output=report/getCampgroundTestReport.html &&
mocha ./test/getRatesTest.js --reporter-options output=report/getRatesTestReport.html &&
mocha ./test/getReservationsTest.js --reporter-options output=report/getReservationsTestReport.html &&
mocha ./test/getAvailableTest.js --reporter-options output=report/getAvailableTestReport.html &&
node -e 'console.log(`Tests end ` + new Date())'
